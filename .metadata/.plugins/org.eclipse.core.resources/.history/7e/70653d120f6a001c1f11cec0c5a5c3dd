package com.sac.practice.Controller;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

import com.sac.practice.Entity.EmpPojo;
import com.sac.practice.Service.EmpServiceImpl;

@RestController
@RequestMapping("/Emp")
public class EmpController {

	@Autowired
	EmpServiceImpl empServiceImpl;
	
	public List<EmpPojo> getAllEmployees(){
		System.out.println("Printing from controller");
		return empServiceImpl.getAllEmployees()
		;
		
	}
	
}
